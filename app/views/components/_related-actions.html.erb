<%
  links ||= []
%>
<% if links.any? %>
  <div class="app-c-related-actions">
    <ul class="govuk-body govuk-body-s app-c-related-actions__links">
      <% links.each do |l| %>
        <% if l[:label] && l[:link_url] %>
          <li class="app-c-related-actions__link">
            <a href="<%= l[:link_url] %>" class="govuk-link">
              <%= l[:label] %>
            </a>
          </li>
        <% end %>
      <% end %>
    </ul>
  </div>
<% end %>
